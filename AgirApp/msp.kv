<MspPrice>:
    Screen:
        BoxLayout:
            orientation: 'vertical'
            MDToolbar:
                title: '   Price'
                MDFlatButton:
                    text: "Back"
                    theme_text_color: "Custom"
                    text_color: 1, 1, 1, 1
                    pos_hint:{"center_y":0.5}
                    on_release: root.manager.current = 'menu'
            AnchorLayout:
                anchor_x : 'center'
                anchor_y : 'center'
                ScrollView:
                    size : self.size
                    GridLayout:
                        size_hint_y : None
                        height : self.minimum_height
                        cols : 3
                        Button:
                            size_hint_y:None
                            text: "Wheat"
